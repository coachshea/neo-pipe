{
  "*.mongo": {
    "type": "mongo",
    "npipe_start": "mongo",
    "npipe_ft": "javascript"
  },
  "*.mongo.ls": {
    "type": "mongols",
    "npipe_com": "lsc -cbp | mongo",
    "npipe_ft": "javascript"
  },
  "*.sql": {
    "type": "sql",
    "npipe_start": "sqlite3 ~/data.db",
    "npipe_split": "20new"
  },
  "*.c": {
    "npipe_com": "gcc -S -xc -c -o - -",
    "npipe_ft": "asm"
  }
}
